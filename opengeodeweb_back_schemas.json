          },
          "z_min": {
            "type": "number",
            "description": "Minimum Z coordinate"
          },
          "z_max": {
            "type": "number",
            "description": "Maximum Z coordinate"
          },
          "id": {
            "type": "string",
            "description": "Optional ID for updating existing VOI"
          }
        },
        "required": [
          "name",
          "aoi_id",
          "z_min",
          "z_max"
        ],
        "additionalProperties": false
      },
